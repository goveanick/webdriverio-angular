{
  "name": "admin-automation",
  "version": "1.0.0",
  "description": "Admin automated tests using WebDriverIO TypeScript and Jasmine",
  "scripts": {
    "lint": "tslint --fix -p .",
    "my-build": "func() { npm run test1  \"$1\"  \"$2\"; }; func",
    "precommit": "npm run lint && npm run tsc  && ls",
    "pretest": "npm run tsc",
    "selenium-start": "node_modules/.bin/selenium-standalone start",
    "tsc": "tsc",
    "test": "./node_modules/.bin/wdio ./src/config/conf.ts",
    "alpine-test": "sh alpine-runner.sh"
  },
  "dependencies": {
    "@types/node": "^10.12.18",
    "@wdio/cli": "^5.8.3",
    "dotenv": "^6.0.0",
    "npm": "^6.5.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "typescript": "^3.1.3",
    "wdio": "^3.0.0",
    "webdriverio": "^5.8.3",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@wdio/allure-reporter": "^5.7.11",
    "@wdio/concise-reporter": "^5.2.3",
    "@wdio/dot-reporter": "^5.2.3",
    "@wdio/jasmine-framework": "^5.3.2",
    "@wdio/junit-reporter": "^5.3.1",
    "@wdio/local-runner": "^5.8.3",
    "@wdio/selenium-standalone-service": "^5.2.2",
    "@wdio/spec-reporter": "^5.2.3",
    "@wdio/sync": "^5.3.2",
    "husky": "^1.1.2",
    "request": "^2.88.0",
    "selenium-standalone": "^6.15.3",
    "tslint": "^5.11.0",
    "url-parse": "^1.4.4",
    "wdio-docker-service": "^1.5.0"
  }
}
